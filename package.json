{
  "name": "ember-octicons",
  "version": "4.0.0",
  "description": "Easily add GitHub Primer/Octicons to your Ember project.",
  "keywords": [
    "ember-addon",
    "octicons",
    "icons"
  ],
  "repository": "https://github.com/kpfefferle/ember-octicons",
  "license": "MIT",
  "author": "Kevin Pfefferle",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.25.2",
    "broccoli-funnel": "^3.0.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "glob": "^10.2.2",
    "validate-peer-dependencies": "^2.0.0"
  },
  "peerDependencies": {
    "@primer/octicons": "^19.1.0",
    "ember-source": "^4.0.0 || >= 5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@ember/optional-features": "^2.1.0",
    "@ember/test-helpers": "^4.0.2",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@primer/octicons": "^19.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.0.0",
    "ember-auto-import": "^2.7.4",
    "ember-cli": "~5.11.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-deprecation-workflow": "^3.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^8.2.3",
    "ember-qunit": "^8.1.0",
    "ember-resolver": "^13.0.0",
    "ember-source": "~5.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-svg-jar": "^2.3.3",
    "ember-template-lint": "^6.0.0",
    "ember-try": "^3.0.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.0.0",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-qunit": "^8.1.1",
    "loader.js": "^4.7.0",
    "prettier": "^3.3.3",
    "qunit": "^2.22.0",
    "qunit-dom": "^3.0.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.1.0",
    "webpack": "^5.93.0"
  },
  "resolutions": {
    "string-width": "^4.2.0"
  },
  "engines": {
    "node": "18.* || >= 20"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "18.20.2",
    "yarn": "1.22.22"
  }
}
